Xft.dpi: 166
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.autohint: true
Xft.hintstyle: hintfull
Xcursor.theme: Vanilla-DMZ-AA
Xcursor.size: 48

URxvt.font: xft:Hack:size=14
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.scrollBar_floating: false
URxvt.scrollstyle: rxvt
URxvt.underlineURLs: true
URxvt.clipboard.autocopy: true
URxvt.externalBorder: 0

URxvt.keysym.Control-k: eval:scroll_up 1
URxvt.keysym.Control-j: eval:scroll_down 1
URxvt.keysym.Shift-Control-v: eval:paste_clipboard
URxvt.keysym.Shift-Control-c: eval:selection_to_clipboard
URxvt.keysym.Control-u: eval:scroll_to_top
URxvt.keysym.Control-d: eval:scroll_to_bottom
URxvt.keysym.Control-n: eval:urxvtc

*background: #32302f
*foreground: #ebdbb2
! Black + DarkGrey
*color0:  #282828
*color8:  #928374
! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934
! DarkGreen + Green
*color2:  #98971a
*color10: #b8bb26
! DarkYellow + Yellow
*color3:  #d79921
*color11: #fabd2f
! DarkBlue + Blue
*color4:  #458588
*color12: #83a598
! DarkMagenta + Magenta
*color5:  #b16286
*color13: #d3869b
! DarkCyan + Cyan
*color6:  #689d6a
*color14: #8ec07c
! LightGrey + White
*color7:  #a89984
*color15: #ebdbb2
