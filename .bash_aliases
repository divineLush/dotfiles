# ls
alias ll="ls -al"
alias l="ls -l"

# navigation
alias ..="cd .."
alias ...="cd ../.."
alias .3="cd ../../.."
alias .4="cd ../../../.."
alias .5="cd ../../../../.."

# git
alias gs="git status"
alias ga="git add"
alias gal="git add ."
alias gbr="git branch"
alias gch="git checkout"
alias gc="git commit"
alias gcm="git commit -m"
alias gf="git fetch"
alias gm="git merge"
alias gr="git rebase"
alias gu="git pull origin"
alias gp="git push origin"
alias gt="git tag"

# update debian system
alias upd="sudo apt update && sudo apt upgrade"
